                        <DataTrigger Binding="{Binding Path=Header.ContainsIncludes, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource InverseDiagonalStripeBrush}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalStripeBrush}" />
                        </DataTrigger>
                        <DataTrigger Binding="{Binding Path=Header.ContainsAlwaysEnableds, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource HorizontalStripeBrush}" />
                        </DataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsAlwaysEnableds, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalHorizontalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsNoMatchIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalInverseDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsAlwaysEnableds, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsNoMatchIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource HorizontalDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsAlwaysEnableds, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource HorizontalInverseDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsNoMatchIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource DiagonalInverseDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsAlwaysEnableds, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsNoMatchIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource HorizontalDiagonalInverseDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsNoMatchIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalDiagonalInverseDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsNoMatchIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalDiagonalInverseDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsAlwaysEnableds, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalHorizontalInverseDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsAlwaysEnableds, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsNoMatchIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalHorizontalInverseDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <MultiDataTrigger>
                            <MultiDataTrigger.Conditions>
                                <Condition Binding="{Binding Path=Header.ContainsIgnores, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsAlwaysEnableds, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsNoMatchIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                                <Condition Binding="{Binding Path=Header.ContainsIncludes, RelativeSource={RelativeSource Self}}" Value="True" />
                            </MultiDataTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource VerticalHorizontalDiagonalInverseDiagonalStripeBrush}" />
                        </MultiDataTrigger>

                        <DataTrigger Binding="{Binding Path=Header.SpecialFileState, RelativeSource={RelativeSource Self}}" Value="True">
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource PrimaryVariantBrush}" />
                        </DataTrigger>

                        <Trigger Property="IsExpanded" Value="false">
                            <Setter TargetName="ItemsHost" Property="Visibility" Value="Collapsed" />
                        </Trigger>
                        <Trigger Property="HasItems" Value="false">
                            <Setter TargetName="Expander" Property="Visibility" Value="Hidden" />
                        </Trigger>

                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="HasHeader" Value="false" />
                                <Condition Property="Width" Value="Auto" />
                            </MultiTrigger.Conditions>

                        </MultiTrigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="HasHeader" Value="false" />
                                <Condition Property="Height" Value="Auto" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="PART_Header" Property="MinHeight" Value="21" />
                        </MultiTrigger>
                        <Trigger Property="IsSelected" Value="true">
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource SelectedListItem}" />
                            <Setter Property="Foreground" Value="{StaticResource SelectedListItemForeground}" />
                            <Setter TargetName="Expander" Property="Style" Value="{DynamicResource TreeViewSelectedToggleButtonStyle}" />
                            <Setter TargetName="Foreground" Property="Foreground" Value="{StaticResource SelectedListItemForeground}" />
                        </Trigger>
                        <MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition SourceName="Bd" Property="IsMouseOver" Value="true" />
                                <Condition Property="IsSelected" Value="false" />
                            </MultiTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{StaticResource MouseOverListItem}" />
                        </MultiTrigger>
                        <!--<MultiTrigger>
                            <MultiTrigger.Conditions>
                                <Condition Property="IsSelected" Value="true"/>
                                <Condition Property="IsSelectionActive" Value="false"/>
                            </MultiTrigger.Conditions>
                            <Setter TargetName="Bd" Property="Background" Value="{DynamicResource {x:Static SystemColors.ControlBrushKey}}"/>
